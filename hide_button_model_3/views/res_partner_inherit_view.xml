<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="res_partner_view_form_edit_inherit" model="ir.ui.view">
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="model">res.partner</field>
        <field name="groups_id" eval="[(6, 0, [ref('hide_button_model.group_edit_contact')])]"/>
        <field name="arch" type="xml">

            <xpath expr="//form" position="attributes">
                <attribute name="edit">0</attribute>
                <attribute name="duplicate">0</attribute>
            </xpath>

        </field>
    </record>

    <record id="res_partner_view_form_delete_inherit" model="ir.ui.view">
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="model">res.partner</field>
        <field name="groups_id" eval="[(6, 0, [ref('hide_button_model.group_delete_contact')])]"/>
        <field name="arch" type="xml">

            <xpath expr="//form" position="attributes">
                <attribute name="delete">0</attribute>
            </xpath>

        </field>
    </record>

</odoo>