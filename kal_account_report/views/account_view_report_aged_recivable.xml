<odoo>
  <data>
    <template id="filter_info_template_custom" inherit_id="account_reports.filter_info_template">
      <xpath expr="//div/div[2]/div[5]" position="after">
        <div class="col-12">
            <t t-if="options.get('seller_ids') != None">
                VENDEDORES:
                <t t-set="seller_value" t-value="'All'"/>
                <t t-foreach="options['selected_seller_ids']" t-as="seller_name">
                    <t t-set="seller_value" t-value="''"/>
                    <t t-esc="seller_name"/>
                </t>
                <t t-esc="seller_value"/>
            </t>
        </div>         
      </xpath>
    </template>
