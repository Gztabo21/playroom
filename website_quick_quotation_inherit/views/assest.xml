<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="asset_frontend" inherit_id="website.assets_frontend" name="Frontend">
            <xpath expr="//link[last()]" position="after">
                <link type="text/css" href="/website_quick_quotation_inherit/static/src/css/style.css" rel="stylesheet" />
            </xpath>
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/website_quick_quotation_inherit/static/src/js/website_quick_quotation.js"></script>
            </xpath>
        </template>
    </data>
</odoo>