<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_user_form_inherit" model="ir.ui.view">
        <field name="name">res.users.form.inherit</field>
        <field name="model">res.users</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_users_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@class='oe_title']/group" position="inside">
                <field name="is_seller" />
            </xpath>
        </field>
    </record>
    <data >

    <record id="main_menu" model="website.menu">
        <field name="name">Quick Quotation</field>
         <field name="url">/quotation/quick</field>
        <field name="parent_id" ref="website.main_menu"/>
    </record>
    
    </data>
    
</odoo>