<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <template id="asset_frontend" inherit_id="website.assets_frontend" name="Frontend">
      <xpath expr="//link[last()]" position="after">
        <link type="text/css" href="/website_save_cart/static/lib/bootstrap-sweetalert/sweetalert.css" rel="stylesheet" />
        <link type="text/css" href="/website_save_cart/static/src/css/website_style.css" />
      </xpath>
      <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/website_save_cart/static/lib/bootstrap-sweetalert/sweetalert.min.js"></script>
        <script type="text/javascript" src="/website_save_cart/static/src/js/website_script.js"></script>
      </xpath>
    </template>
  </data>
</odoo>