<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="sale_order_multi_product_view_form" model="ir.ui.view">
		<field name="name">sale.order.multi.product.form</field>
		<field name="model">sale.order.multi.product</field>
		<field name="arch" type="xml">
			<form>

				<group col="2" string="Productos">
					<field name="product_ids" nolabel="1">
						<tree editable="bottom">
							<field name="name" readonly="1"/>
							<field name="product_qty_aux" />
						</tree>
					</field>
				</group>

				<footer>
					<button name="load_order_line" string="Cargar Productos" type="object" class="oe_highlight"/>
					o
					<button string="Cancel" class="oe_link" special="cancel"/>
				</footer>
		
			</form>
		</field>
	</record>
	
</odoo>